@page
@model PhotoAlbumShare.Models.GalleryIndexModel
@{
}
<div>
    <div class="gallery-content">
        @foreach (var image in Model.Images)
        {
            <div class="gallery-image"></div>
            <span>image.Title</span>
            <ul>
                @foreach (var tag in image.Tags)
                {
                    <li>
                        @tag.Description
                    </li>
                }
            </ul>
        }
    </div>
</div>